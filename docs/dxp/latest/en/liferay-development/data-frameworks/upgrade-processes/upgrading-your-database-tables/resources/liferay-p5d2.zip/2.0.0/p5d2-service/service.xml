<?xml version="1.0"?>
<!DOCTYPE service-builder PUBLIC "-//Liferay//DTD Service Builder 7.4.0//EN" "http://www.liferay.com/dtd/liferay-service-builder_7_4_0.dtd">

<service-builder dependency-injector="ds" package-path="com.acme.p5d2" short-no-such-exception-enabled="false">
	<namespace>P5D2</namespace>
	<entity local-service="true" name="P5D2Entry" remote-service="false">

		<!-- PK fields -->

		<column name="p5d2EntryId" primary="true" type="long" />

		<!-- Other fields -->

		<column name="able" type="Date" />
		<column name="bar" type="String" />
		<column name="charlie" type="String" />
	</entity>
</service-builder>